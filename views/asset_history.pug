  #layoutSidenav_content
    main
      .container-fluid.px-4
        // Button trigger modal
        //
          <button
          type="button"
          id="button"
          class="btn btn-primary"
          data-bs-toggle="modal"
          data-bs-target="#exampleModal"
          >
          Asset History
          </button>
        // Modal
        #exampleModal.modal.fade(tabindex='-1' aria-labelledby='exampleModalLabel' aria-hidden='true')
          .modal-dialog
            .modal-content
              .modal-header
                h5#exampleModalLabel.modal-title
                  | Asset History Table
                button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
              .modal-body
                table#example.display(style='width: 100%')
                  h4 Asset History Form
                  .container
                    form#formvalidate.row.g-2(role='form' method='PUT')
                      .col-12
                        label.form-label(for='assetName') Asset Name
                        input#assetName.assetName.form-control(type='text' placeholder='Enter the asset name' required='')
                        .error
                      .col-12
                        label.form-label(for='issueDate') Issue Date
                        input#issueDate.issueDate.form-control(type='text' placeholder='Enter the Issue Date of the  Product' required='')
                        .error
                      .col-12
                        label.form-label(for='returnDate') Return Date
                        input#returnDate.returnDate.form-control(type='date' placeholder='Enter the Return Date of the Product' required='')
                        .error
                      .col-12
                        label.form-label(for='model') Scrap Date
                        input#scrapDate.scrapDate.form-control(type='date' placeholder='Enter the Scrap Date of the product' required='')
                        .error
                      .modal-footer
                        input#id(type='hidden')
                        button.btn.btn-secondary(type='button' data-bs-dismiss='modal')
                          | Close
                        button#submitBtn.btn.btn-primary(type='submit' data-bs-dismiss='modal' onclick='sample()')
                          | Submit
      div
        .container.col
          table#asset_history.table.table-bordered.hover
            thead
              tr
                th Id
                th Asset Name
                th Issue Date
                th Return Date
                th Scrap Date
                th EmployeeId
