  #layoutSidenav_content
    main
      .container-fluid.px-4
        // Button trigger modal
        button#button.btn.btn-primary(type='button' data-bs-toggle='modal' data-bs-target='#exampleModal')
          | Add Asset
        // Modal
        #exampleModal.modal.fade(tabindex='-1' aria-labelledby='exampleModalLabel' aria-hidden='true')
          .modal-dialog
            .modal-content
              .modal-header
                h5#exampleModalLabel.modal-title Asset Master Table
                button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
              .modal-body
                table#example.display(style='width:100%')
                  // <h4>Asset Details Form</h4>
                  .container
                    form#formvalidate.row.g-2(role='form' method='PUT')
                      .col-12
                        label.form-label(for='serialNumber') Serial Number
                        input#serialNumber.serialNumber.form-control(type='number' placeholder='Enter the Serial Number' required='')
                        .error
                      .col-12
                        label.form-label(for='assetName') Asset Name
                        input#assetName.assetName.form-control(type='text' placeholder='Enter the Asset name' required='')
                        .error
                      .col-12
                        label.form-label(for='make') Make
                        input#make.make.form-control(type='text' placeholder='Enter the Manfacturer details' required='')
                        .error
                      .col-12
                        label.form-label(for='model') Model
                        input#model.model.form-control(type='text' placeholder='Enter the Model Name' required='')
                        .error
                      .col-12
                        label.form-label(for='cost') Cost
                        input#cost.cost.form-control(type='number' placeholder='Enter the Cost of the Product' required='')
                        .error
                      //
                        <div class="col-12">
                        <label for="model" class="form-label">EmployeeId</label>
                        <input
                        type="text"
                        class="employeeId form-control"
                        id="employeeId"
                        placeholder="Enter the employeeId"
                        required
                        />
                        <div class="error"></div>
                        </div>
                      .modal-footer
                        input#id(type='hidden')
                        button.btn.btn-secondary(type='button' data-bs-dismiss='modal')
                          | Close
                        button#submitBtn.btn.btn-primary(type='submit' data-bs-dismiss='modal' onclick='sample()')
                          | Submit
                        button#editBtn.btn.btn-primary(type='submit' data-bs-dismiss='modal' onclick='edit()')
                          | Submit
      div
        .container.col
          table#asset_master.table.table-bordered.hover
            thead
              tr
                th Id
                th Serial Number
                th Asset Name
                th Make
                th Model
                th Cost
                th EmployeeId
                th
      // Issue Asset
      #issueAssetModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='issueAssetModalLabel' aria-hidden='true')
        .modal-dialog(role='document')
          .modal-content
            .modal-header
              h5#issueAssetModalLabel.modal-title Issue Asset
              button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
            .modal-body
              .col-12
                label.form-label(for='employeeName') Employee Name
                select#employee.employee.form-control(name='employee' required='')
                  option(value='' disabled='' selected='') Select Employee Name
                .error
              .col-12
                label.form-label(for='assettype')
                  | Asset Type
                input#assettype.assettype.form-control(type='text' name='assettype' placeholder='Enter the Asset Name' required='')
                .error
              .col-12
                label.form-label(for='issueDate') Issue Date
                input#issueDate.issueDate.form-control(type='date' name='issueDate' placeholder='Enter the Issue Date for the Product' required='')
                .error
              .form-group
                label(for='reason') Reason:
                textarea#reason.form-control.reason(rows='3')
            .modal-footer
              input#asset_id(type='hidden' name='asset_id')
              button.btn.btn-secondary(type='button' data-bs-dismiss='modal')
                | Close
              button#submitBtn.btn.btn-primary(type='submit' data-bs-dismiss='modal' onclick='simple()')
                | Submit
      // Return Asset
      #returnAssetModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='returnAssetModalLabel' aria-hidden='true')
        .modal-dialog(role='document')
          .modal-content
            .modal-header
              h5#returnAssetModalLabel.modal-title Return Asset
              button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
            .modal-body
              .col-12
                label.form-label(for='returndate') Return Date
                input#returndate.returndate.form-control(type='date' placeholder='Enter the Return Date for the Product' required='')
                .error
              .form-group
                label(for='reason') Reason
                textarea#reason.form-control.reason(rows='3')
            .modal-footer
              input#id(type='hidden')
              button.btn.btn-secondary(type='button' data-bs-dismiss='modal')
                | Close
              button#submitBtn.btn.btn-primary(type='submit' data-bs-dismiss='modal' onclick='model()')
                | Submit
      // Scrap Asset
      #scrapAssetModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='scrapAssetModalLabel' aria-hidden='true')
        .modal-dialog(role='document')
          .modal-content
            .modal-header
              h5#scrapAssetModalLabel.modal-title Scrap Asset
              button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
            .col-12
              label.form-label(for='scrapDate') Scrap Date
              input#scrapDate.scrapDate.form-control(type='date' placeholder='Enter the Date for the Product' required='')
              .error
            .modal-body
              .col-12
                label.form-label(for='defects') Defects
                select#defects.defects.form-control(type='text' name='defects' required='')
                  option(value='' disabled='' selected='') Select Reason
                  option(value='Product Damaged') Product Damaged
                  option(value='Product Not Work') Product Not Work
                .error
              .form-group
                label(for='reason') Reason
                textarea#reason.form-control.reason(rows='3')
            .modal-footer
              input#id(type='hidden')
              button.btn.btn-secondary(type='button' data-bs-dismiss='modal')
                | Close
              button#submitBtn.btn.btn-primary(type='submit' data-bs-dismiss='modal' onclick='scrap()')
                | Submit
