  <div class="modal fade" id="exampleButton" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
  <div class="modal-content">
  <div class="modal-header">
  <h5 class="modal-title" id="exampleModalLabel">Employee Table</h5>
  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
  </div>
  <div class="modal-body">
  <table id="example" class="display" style="width:100%">
  <h1>Employee Details Form</h1>
  <div class="container">
  <form id="form" class="row g-2 needs-validation" novalidate>
  <div class="col-12">
  <label for="employeeName" class="form-label" >name</label>
  <input
  type="text"
  class="names form-control"
  id="name"
  placeholder="Enter the Employee name"
  required
  />
  <div class="error"></div>
  </div>
  <div class="col-12">
  <label for="dept" class="form-label">designation</label>
  <input type="text" class=" designation form-control" list="datalistOptions" id="designation" placeholder="Type to search...">
  <datalist id="designation">
  <option value="Human Resources">
  <option value="IT">
  <option value="Sales">
  <option value="Marketing">
  <option value="Finance">
  </datalist>
  <div class="error"></div>
  </div>
  <div class="col-md-6">
  <label for="date" class="form-label">dob</label>
  <input type="date" id="dob" class="dobs form-control" />
  <div class="error"></div>
  </div>
  <div class="col-md-6">
  <label for="mobileNumber" class="form-label">mobile</label>
  <input
  type="number"
  id="mobile"
  class=" mobile form-control"
  placeholder="Enter your phone number"
  />
  <div class="error"></div>
  </div>
  <div class="col-md-6">
  <label for="salary" class="form-label">email</label>
  <input
  type="email"
  id="email"
  class="email form-control"
  placeholder="Enter you email id"
  />
  <div class="error"></div>
  </div>
  <div class="col-md-6">
  <label for="experience" class="form-label">branch</label>
  <input
  type="text"
  class=" branch form-control"
  id="branch"
  placeholder="Enter the branch"
  />
  <div class="error"></div>
  </div>
  <div class="col-12">
  <label for="status" class="form-label">status</label>
  <input type="text" class=" status form-control" id="status" placeholder="Display the Status of the Employee" />
  <div class="error"></div>
  </div>
  </form>
  </div>
  </table>
  <div class="modal-footer">
  <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
  <button type="submit" class="btn btn-primary"  onclick="sample()">Submit</button>
  </div>
  </div>
  </div>
  </div>
  </div>
  </div>
